<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window  xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.xformix.comicbook.reports.rpt_series.Rpt_seriesEdit"
        datasource="qryrptseriesDs"
        focusComponent="fieldGroup"
        messagesPack="com.xformix.comicbook.reports.rpt_series">
    <dialogMode height="600"
                width="800"/>
<layout spacing="true" >
    <scrollBox width="100%" height="100%" spacing="false" margin="false">
    <vbox id="TOP_LEVEL" width="748.8px" height="100%"  stylename="report" >
      <grid spacing="true" id="ReportHeader"  height="67.2px" width="316.8px"  >
        <columns count="1"/>
        <rows>
          <row>
          <label  id="Label10" value="Series Report"  height="45.0px" row="0" column="0"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
          </row>
        </rows>
      </grid>
      <grid spacing="true" id="PageHeaderSection"  height="31.199999px" width="748.8px"  >
        <columns count="3"/>
        <rows>
          <row>
            <label  id="Sum_Of_Cost_Label" value="Series"  height="20.0px" row="0" column="0"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
            <label  id="Avg_Of_Cost_Label" value="# of Issues"  height="20.0px" row="0" column="1"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
            <label  id="Label16" value="Total Cost"  height="25.0px" row="0" column="2"  align="MIDDLE_RIGHT" htmlEnabled="false" stylename="access-label"/>
          </row>
          <row>
            <label  id="Line13" height="1.0px" width="740.0px" colspan="3" row="1" column="0"  align="MIDDLE_CENTER" value=" " stylename="report-line" />
          </row>
        </rows>
      </grid>
      <grid spacing="true" id="Detail"  height="19.199999px" width="744.0px"  >
        <columns count="3"/>
        <rows>
          <row>
            <textField  id="Sum_Of_Cost" property="series"  datasource="qryrptseriesDs"  height="20.0px" width="310.0px" row="0" column="0"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
            <textField  id="Avg_Of_Cost" property="countofcomicbooknum"  datasource="qryrptseriesDs"  height="20.0px" width="290.0px" row="0" column="1"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
            <textField  id="SumOfCost" property="sumofcost"  datasource="qryrptseriesDs"  height="20.0px" width="130.0px" row="0" column="2"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
          </row>
        </rows>
      </grid>
      <grid spacing="true" id="PageFooterSection"  height="43.2px" width="811.2px"  >
        <columns count="3"/>
        <rows>
          <row>
            <label  id="Line14" height="1.0px" width="740.0px" colspan="3" row="0" column="0"  align="MIDDLE_CENTER" value=" " stylename="report-line" />
          </row>
          <row>
            <textField  id="Text11" height="20.0px" width="310.0px" row="1" column="0"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
            <textField  id="Text12" height="20.0px" width="430.0px" colspan="2" row="1" column="1"  stylename="access-textfield"  align="MIDDLE_LEFT"/>
          </row>
        </rows>
      </grid>
</vbox>
</scrollBox>
</layout>
</window>
